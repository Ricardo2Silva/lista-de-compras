  },
  "homepage": "https://github.com/Ricardo2Silva/lista-de-compras#readme",
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "dotenv": "^8.2.0",
    "rimraf": "^3.0.2"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "dotenv-expand": "^5.1.0",
    "ejs": "^3.0.1",
    "express": "^4.17.1",
